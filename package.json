{
  "name": "weather-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-watch:component": "npx @tailwindcss/cli -i ./src/styles/input/component.input.css -o ./src/styles/component.css --watch",
    "build-watch:custom": "npx @tailwindcss/cli -i ./src/styles/input/custom.input.css -o ./src/styles/custom.css --watch",
    "build-watch:index": "npx @tailwindcss/cli -i ./src/styles/input/index.input.css -o ./src/styles/index.css --watch",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "@tanstack/react-virtual": "^3.13.8",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@stylistic/eslint-plugin-ts": "^1.7.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.3.0",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  }
